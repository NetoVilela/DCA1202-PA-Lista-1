// Percorrerá o vetor e imprimirá na tela cada elemento
int main(){
  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
    printf("%d ",*(vet+i));
  }
}


// Percorrerá o vetor e imprimirá a posição de memória de cada elemento dele
int main(){
  int vet[] = {4,9,13};
  int i;
  for(i=0;i<3;i++){
  printf("%X ",vet+i);
  }
}


